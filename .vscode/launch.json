{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Streamlit Debug",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/run.py",
        "args": ["run"],
        "console": "integratedTerminal",
        "env": {
          "STREAMLIT_SERVER_PORT": "8501",
          "STREAMLIT_SERVER_ADDRESS": "0.0.0.0"
        },
        "justMyCode": true
      }
    ]
  }
